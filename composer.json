{
    "name": "core23/gdpr-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a GDPR conform cookie information inside the sonata-project.",
    "keywords": [
        "symfony",
        "gdpr",
        "dsgvo",
        "cookie",
        "sonata",
        "block",
        "widget",
        "bundle"
    ],
    "homepage": "https://core23.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Gripp",
            "email": "mail@core23.de"
        }
    ],
    "require": {
        "php": "^7.2",
        "sonata-project/admin-bundle": "^3.35",
        "sonata-project/block-bundle": "^3.16.1",
        "sonata-project/core-bundle": "^3.13.5",
        "symfony/dependency-injection": "^3.4 || ^4.2",
        "symfony/expression-language": "^3.4 || ^4.2",
        "symfony/framework-bundle": "^3.4 || ^4.2",
        "symfony/options-resolver": "^3.4 || ^4.2",
        "symfony/twig-bundle": "^3.4 || ^4.2"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.2",
        "sllh/composer-lint": "^1.0"
    },
    "suggest": {
        "core23/shariff-bundle": "If you need a GDPR conform social media widget integration"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Core23\\GDPRBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Core23\\GDPRBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "@composer bin all update --ansi"
        ]
    }
}
