{
  "name": "core23/gdpr-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides a GDPR conform cookie information inside the sonata-project.",
  "keywords": [
    "symfony",
    "gdpr",
    "dsgvo",
    "cookie",
    "sonata",
    "sonata-block",
    "block",
    "widget",
    "bundle"
  ],
  "homepage": "https://core23.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Gripp",
      "email": "mail@core23.de"
    }
  ],
  "require": {
    "php": "^7.3",
    "sonata-project/block-bundle": "^4.0",
    "sonata-project/form-extensions": "^1.1",
    "symfony/dependency-injection": "^4.2",
    "symfony/expression-language": "^4.2",
    "symfony/framework-bundle": "^4.2",
    "symfony/options-resolver": "^4.2",
    "symfony/twig-bundle": "^4.2"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.3",
    "localheinz/composer-normalize": "^1.3"
  },
  "suggest": {
    "core23/shariff-bundle": "If you need a GDPR conform social media widget integration"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Core23\\GDPRBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Core23\\GDPRBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@composer bin all install --ansi"
    ],
    "post-update-cmd": [
      "@composer bin all update --ansi"
    ]
  }
}
